{
  "compilerOptions": {
    "outDir": "./dist/",
     "module": "esnext",
    "target": "es2017",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks":false, 
    "esModuleInterop": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
  
    "jsx": "react",
    "jsxFactory": "h",
    "allowJs": true
  },
   "exclude": [
    "dist",
    "node_modules"
  ],

}
